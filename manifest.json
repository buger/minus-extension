{
   "name": "Minus",
   "version": "0.1.1",
   "description": "Min.us - Share simply",
   "icons": {
      "16": "images/logo_37.png",
      "128": "images/logo_128.png"
   },
   "browser_action": {
     "default_icon": "images/logo_37.png", 
     "popup": "popup.html"        
   },
   "content_scripts": [{
      "css": [ "css/page.css"],
      "js": ["js/jquery.min.js", "js/jquery-ui.min.js", "js/browser_api.js", "js/content_script.js"],
      "matches": ["http://*/*","https://*/*"]
   },
   {
      "js": ["js/minus_auth.js"],
      "matches": ["http://min.us/*"]
   }],
   "background_page": "background.html",
   "options_page": "options.html",
   "permissions": ["contextMenus", "tabs", "http://*/", "https://*/"]
}
